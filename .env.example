# OpenAI 兼容的 LLM 配置
OPENAI_API_KEY=your_api_key_here
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_MODEL=gpt-4o

# 代理配置（可选）
HTTP_PROXY=
HTTPS_PROXY=

# 日志级别
LOG_LEVEL=INFO

# Autogen 兼容性补丁
# 用于修复某些 OpenAI 兼容服务器(如 Gemini API 转发)不支持 additionalProperties 字段的问题
# 如果使用标准 OpenAI API,建议设置为 false
# 如果使用 Gemini 代理或其他不支持完整 JSON Schema 的服务器,设置为 true
# 默认: false (不启用)
# 当前建议: true (因为使用 Gemini API 转发服务器)
ENABLE_AUTOGEN_PATCH=true

# Playwright 配置
HEADLESS=false
BROWSER_TIMEOUT=30000

